#include <iostream>
#include <iomanip>
#include <cmath>

using namespace std;

int main() {
    int a, b;
    float h, c, d;

    cout << "Enter c =";
    cin >> a;
    cout << "Enter x =";
    cin >> b;
    cout << "Enter h =";
    cin >> h;
  

    float y, x = a;

    while (x <= b) {
        y = sqrt(x) + cos(x) - 3;
        cout << setw(10) << "x=" << x << setw(10) << "y=" << y << endl;
        x += h;
    }

    return 0;
}





